
abstract class Vehicle {
    Vehicle() {
        System.out.println("Vehicle constructor called");
    }

    abstract void start();
}

class Bike extends Vehicle {
    void start() {
        System.out.println("Bike starts with kick");
    }
}

class Main {
    public static void main(String[] args) {
        Vehicle v = new Bike();
        v.start();
    }
}
