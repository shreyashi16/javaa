

class Person {

    String name = "Rahul";

    void showName() {
        System.out.println("Name: " + name);
    }
}

class Student extends Person {

    int rollNo = 101;

    void showRoll() {
        System.out.println("Roll No: " + rollNo);
    }

    public static void main(String[] args) {
        Student s = new Student();
        s.showName();   // parent method
        s.showRoll();   // child method
    }
}
