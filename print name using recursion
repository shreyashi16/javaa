import java.util.*;
class Solution {
    static void printName(int n, String name) {
    if(n==0){
return;
    }
printName(n-1, name);
System.out.println(name);
    }
 public static void main (String [] args){
Scanner sc = new Scanner(System.in);
int n = sc.nextInt();
String name = sc.nextLine();
printName(n,name);

}
}
